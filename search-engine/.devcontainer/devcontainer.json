{
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "forwardPorts": [
    5000
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-toolsai.jupyter-renderers"
      ]
    }
  },
  "mounts": [
    "source=${localEnv:HOME}/data,target=/mnt/data,type=bind,consistency=cached"
  ]
}